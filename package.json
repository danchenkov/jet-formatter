{
    "name": "jet-formatter",
    "displayName": "Jet Template Formatter",
    "description": "Formatter for Go HTML Jet template with front matter",
    "version": "0.0.1",
    "author": "Alexei Danchenkov",
    "publisher": "alexeidanchenkov",
    "license": "MIT",
    "engines": {
        "vscode": "^1.32.0"
    },
    "keywords": [
        "Jet",
        "Formatters"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/danchenkov/jet-formatter"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onLanguage:jet"
    ],
    "main": "./dist/extension",
    "contributes": {
        "languages": [
            {
                "id": "jet",
                "aliases": [
                    "Jet"
                ],
                "extensions": [
                    "jet",
                    "html"
                ]
            }
        ]
    },
    "scripts": {
        "postinstall": "tsc"
    },
    "devDependencies": {
        "typescript": "5.4.2"
    },
    "dependencies": {
        "@types/vscode": "^1.88.0"
    }
}