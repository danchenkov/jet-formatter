{
    "name": "jet-formatter",
    "displayName": "Jet Template",
    "description": "Formatter for Go HTML Jet template with front matter",
    "version": "0.0.26",
    "author": "Alexei Danchenkov",
    "publisher": "alexeidanchenkov",
    "license": "MIT",
    "engines": {
        "vscode": "^1.96.0"
    },
    "keywords": [
        "Jet",
        "Formatters"
    ],
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:jet"
    ],
    "contributes": {
        "languages": [
            {
                "id": "jet",
                "aliases": [
                    "Jet Template Engine",
                    "Jet Template",
                    "Jet",
                    "jet"
                ],
                "extensions": [
                    "jet",
                    "html"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jet",
                "scopeName": "text.html.jet",
                "path": "./syntaxes/jet.tmLanguage.json"
            }
        ]
    },
    "main": "./dist/src/extension.js",
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "build": "vsce package; mv *.vsix ~/Downloads"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/danchenkov/jet-formatter.git"
    },
    "devDependencies": {
        "@types/vscode": "^1.96.0"
    },
    "packageManager": "pnpm@9.1.0+sha512.67f5879916a9293e5cf059c23853d571beaf4f753c707f40cb22bed5fb1578c6aad3b6c4107ccb3ba0b35be003eb621a16471ac836c87beb53f9d54bb4612724"
}